{{#each this}}
<div>
    <h1>{{name}}</h1>    
    <div class="country-img">
        <img src="{{flag}}" width="400" alt="{{name}}" />
    </div>
    <ul class="list">
        <li>Capital: {{capital}}</li>
        <li>Population: {{population}}</li>
        <li> Languages:
            <ul>
                {{#each languages}}
                <li>{{this.name}}</li>
                {{/each}}
            </ul>
        </li>
    </ul>

</div>
{{/each}}